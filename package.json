{
  "name": "leasot-issues",
  "version": "0.1.0",
  "description": "Parse and output github issues references from your code comments (leasot plugin)",
  "main": "dist/index.js",
  "repository": "https://github.com/vadistic/issue-fixer",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "copy": "shx cp ./dist/parsers/* ./node_modules/leasot/dist/lib/parsers/",
